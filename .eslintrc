{
  "parser": "babel-eslint",
  "env": {
    "browser": true,
    "node": true,
    "mocha": true
  },
  "globals": {
    "ENV": false
  },
  "extends": "airbnb",
  "plugins": ["react-hooks"],
  "rules": {
    "camelcase": 0,
    "no-shadow": ["error"],
    "import/no-unresolved": 0,
    "react/prop-types": 0,
    "react/jsx-no-bind":0,
    "react/jsx-props-no-spreading": 0,
    "jsx-a11y/click-events-have-key-events": 0,
    "intjsx-a11y/click-events-have-key-events": 0,
    "jsx-a11y/anchor-is-valid": 0,
    "jsx-a11y/alt-text": 0,
    "arrow-parens": 0,
    "linebreak-style": 0,
    "no-debugger": 0,
    "no-alert": 0,
    "no-console": 0,
    "react/forbid-prop-types": 0,
    "no-useless-return": 0,
    "no-use-before-define": 0,
    "jsx-a11y/no-static-element-interactions": 0,
    "react/jsx-one-expression-per-line": 0,
    "no-useless-escape": 0,
    "react/no-find-dom-node": 0,
    "no-param-reassign": 0,
    "max-len": 0,
    "react/sort-comp": 0,
    "indent": 0,
    "quotes": 0,
    "comma-dangle": ["off"],
    "no-unused-vars": ["warn"],
    "object-curly-spacing": ["off"],
    "padded-blocks": ["off"],
    "react/jsx-closing-bracket-location": ["off"],
    "react/jsx-filename-extension": [
      1,
      {
        "extensions": [".js", ".jsx"]
      }
    ],
    "react/jsx-space-before-closing": ["off"],
    "react/prefer-stateless-function": ["off"],
    "no-underscore-dangle": [
      "error",
      {
        "allowAfterThis": true
      }
    ],
    "import/no-extraneous-dependencies": ["off"],
    "import/extensions": "off",
    "global-require": "off"
  }
}
